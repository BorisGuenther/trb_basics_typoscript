#Setup Meta-Tags
page.meta.viewport = {$trb_basics_typoscript.head.meta.viewport}
page.meta.Content-Type = {$trb_basics_typoscript.head.meta.contenttype}
page.meta.X-UA-Compatible = IE=edge

[globalVar = LIT:0<{$trb_basics_typoscript.head.meta.page_meta}]
	page.meta.author.field = author
	page.meta.description.field = description
	page.meta.keywords.field = keywords
[global]